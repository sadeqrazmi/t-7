#include<iostream>

using namespace std;



int f ( int x)
{
    if ((x==1)||(x==2))
        return 1;
    else if (x==0)
        return 0;
    else
        return f(x-1)+f(x-2);
}

main()
{
int x=1,fibo=f(x),n;		
	cout<<"tedad adad fibonacci morad nazar ra vared konid"<<endl;
	cin>> n;

	for(int i;i<n;i++){
		
		
	cout<<fibo<<"  ";
	x++;
	fibo=f(x);
	}
	
	

	
	
	return 0;
	
	
}
